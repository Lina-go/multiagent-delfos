# Delfos Multi-Agent System Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# Azure AI Foundry (REQUIRED)
# =============================================================================
# Get this from: Azure AI Foundry → Tu Proyecto → Overview → Project endpoint
AZURE_AI_PROJECT_ENDPOINT=https://tu-proyecto.services.ai.azure.com/

# Model deployment name (from Azure AI Foundry → Deployments)
AZURE_AI_MODEL_DEPLOYMENT_NAME=gpt-4.1-mini

# =============================================================================
# MCP Server (Your deployed Azure Functions)
# =============================================================================
MCP_SERVER_URL=https://func-mcp-n2z2m7tmh3kvk.azurewebsites.net/mcp

# =============================================================================
# Power BI (Optional - for visualization)
# =============================================================================
POWERBI_WORKSPACE_ID=
POWERBI_REPORT_ID=

# =============================================================================
# Application Settings
# =============================================================================
DEBUG=false
LOG_LEVEL=INFO

# =============================================================================
# AUTHENTICATION
# =============================================================================
# Uses Azure CLI authentication (az login)
# No API keys needed - just run: az login
